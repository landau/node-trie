{
  "name": "@landau/trie",
  "version": "1.0.0",
  "description": "A trie implementation for NodeJS",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist/**",
    "build": "npm run -s clean && tsc",
    "watch": "tsc --watch",
    "lint": "eslint . --ext .ts",
    "test": "nyc -s node src/test.ts",
    "prepublishOnly": "npm run -s clean && tsc",
    "posttest": "npm run -s lint && npm run -s view-cov && npm run -s check-cov",
    "check-cov": "nyc check-coverage --lines 100 --functions 100 --branches 100",
    "html-cov": "nyc report --reporter=html",
    "view-cov": "nyc report",
    "browse-cov": "npm run-s html-cov && open coverage/index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/landau/node-trie.git"
  },
  "keywords": [
    "trie",
    "string",
    "search",
    "datastructure"
  ],
  "author": "Trevor Landau",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/landau/node-trie/issues"
  },
  "homepage": "https://github.com/landau/node-trie#readme",
  "engines": {
    "node": ">= 8.0.0"
  },
  "files": [
    "dist/index.*",
    "src/index.ts",
    "REaDME.md"
  ],
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ]
  },
  "devDependencies": {
    "@landau/eslint-config": "1.0.2",
    "@typescript-eslint/eslint-plugin": "1.6.0",
    "@typescript-eslint/parser": "1.6.0",
    "eslint": "5.16.0",
    "eslint-config-prettier": "4.1.0",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-promise": "4.1.1",
    "nyc": "13.3.0",
    "prettier": "1.16.4",
    "ts-node": "8.0.3",
    "typescript": "3.4.2"
  },
  "dependencies": {
    "@types/node": "10.14.4"
  }
}
