{
  "name": "@landau/trie",
  "version": "1.0.0",
  "description": "A trie implementation for NodeJS",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist/**",
    "build": "npm run -s clean && tsc",
    "watch": "tsc --watch",
    "test": "nyc -s node src/test.ts",
    "prepublishOnly": "npm run -s clean && tsc",
    "posttest": "eslint . --ext .ts && npm run -s view-cov && npm run -s check-cov",
    "check-cov": "nyc check-coverage --lines 100 --functions 100 --branches 100",
    "html-cov": "nyc report --reporter=html",
    "view-cov": "nyc report",
    "browse-cov": "npm run-s html-cov && open coverage/index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/landau/node-trie.git"
  },
  "keywords": [
    "trie",
    "string",
    "search",
    "datastructure"
  ],
  "author": "Trevor Landau",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/landau/node-trie/issues"
  },
  "homepage": "https://github.com/landau/node-trie#readme",
  "engines": {
    "node": ">= 8.0.0"
  },
  "files": [
    "dist/index.js",
    "dist/index.js.map",
    "dist/index.d.ts",
    "src/index.ts",
    "REaDME.md"
  ],
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ]
  },
  "devDependencies": {
    "eslint": "5.9.0",
    "eslint-config-prettier": "3.3.0",
    "eslint-config-standard": "12.0.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-node": "8.0.0",
    "eslint-plugin-prettier": "3.0.0",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-standard": "4.0.0",
    "eslint-plugin-typescript": "0.14.0",
    "nyc": "13.1.0",
    "prettier": "1.15.2",
    "ts-node": "7.0.1",
    "typescript": "3.1.6",
    "typescript-eslint-parser": "21.0.1"
  },
  "dependencies": {
    "@types/node": "10.12.10"
  }
}
